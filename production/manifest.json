{
    "name" : "VideoNot.es",
    "version" : "0.3",
    "manifest_version" : 2,
    "description" : "The easiest way to take notes from videos. Everything synchronized!",
    "container" : "GOOGLE_DRIVE",
    "api_console_project_id" : "653335932456",
    "icons": {
        "128": "icon-128.png"
    },
    "app" : {
        "urls": [
            "http://www.videonot.es/",
            "http://www.videonot.es/edit/"
        ],
        "launch" : {
            "web_url" : "http://www.videonot.es/edit/"
        }
    },
    "gdrive_mime_types": {
        "http://drive.google.com/intents/opendrivedoc": [
            {
                "type": ["application/vnd.unishared.document", "application/vnd.google.drive.ext-type.videonotes"],
                "href": "http://www.videonot.es/edit/",
                "title" : "Open",
                "disposition" : "window"
            }
        ]
    },
    "permissions": [
        "https://www.udacity.com/course/viewer/*",
        "http://videonot.es/svc"
    ],
    "web_accessible_resources": [ "css/styles.css" ],
    "content_scripts": [
        {
            "matches": ["https://www.udacity.com/course/viewer/*"],
            "css": ["css/styles.css"],
            "js": [
                "lib/jquery-1.10.1.min.js", "lib/ace/ace.min.js", 
                "js/videonotes-chrome.min.js"
            ],
            "run_at": "document_end"
        }
    ]
}